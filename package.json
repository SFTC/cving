{
  "name": "cving",
  "version": "0.1.9",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build && webpack",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release": "npm run postversion && npm run build && npm run updateversion && npm publish",
    "postversion": "dumi assets",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "updateversion": "git commit -am $npm_package_version && git tag $npm_package_version && git push && git push --tags",
    "test:coverage": "umi-test --coverage"
  },
  "repository": "https://github.com/SFTC/cving",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "/lib",
    "/es",
    "/dist",
    "assets.json"
  ],
  "dumiAssets": "assets.json",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "antd": "^4.6.3",
    "better-js-lib": "^2.0.8",
    "lodash": "^4.17.20",
    "umi-request": "^1.3.5"
  },
  "devDependencies": {
    "@ant-design/pro-table": "^2.8.1",
    "@types/classnames": "^2.2.10",
    "@types/react-copy-to-clipboard": "^4.3.0",
    "@types/react-syntax-highlighter": "^13.5.0",
    "@umijs/fabric": "^2.2.2",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.0.5",
    "babel-loader": "^8.1.0",
    "dumi": "^1.1.0-rc.3",
    "father-build": "^1.18.4",
    "gh-pages": "^3.0.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.0.7",
    "mini-css-extract-plugin": "^0.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "prettier": "^1.19.1",
    "react-copy-to-clipboard": "^5.0.2",
    "react-syntax-highlighter": "^15.3.0",
    "terser-webpack-plugin": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "yorkie": "^2.0.0"
  },
  "peerDependencies": {
    "react": "16.x"
  }
}
