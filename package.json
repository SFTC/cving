{
  "name": "cving",
  "version": "0.0.1",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d dist",
    "build": "father-build && webpack",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release": "npm run build && npm publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "repository": "https://github.com/SFTC/cving",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "/lib",
    "/es",
    "/dist"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "antd": "^4.6.3",
    "better-js-lib": "^2.0.8"
  },
  "devDependencies": {
    "@ant-design/pro-table": "latest",
    "@types/classnames": "^2.2.10",
    "@umijs/fabric": "^2.2.2",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.0.5",
    "babel-loader": "^8.1.0",
    "dumi": "^1.0.9",
    "father-build": "^1.18.4",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "mini-css-extract-plugin": "^0.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "prettier": "^1.19.1",
    "terser-webpack-plugin": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "yorkie": "^2.0.0"
  },
  "peerDependencies": {
    "react": "16.x"
  }
}
